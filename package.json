{
  "name": "opencaves-app",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "dev": "concurrently --kill-others --prefix none \"firebase --debug emulators:start --import=./.emulator-data\"  \"craco  start\"",
    "dev:serve": "craco start",
    "serve": "serve -p 4000 -s build",
    "build": "craco  build",
    "deploy": "firebase deploy",
    "deploy:beta": "firebase hosting:channel:deploy beta",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": ">= 18.16.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not safari < 10",
      "not chrome < 51",
      "not android < 5",
      "not ie < 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "safari >= 14"
    ]
  },
  "dependencies": {
    "@capacitor/share": "^8.0.0",
