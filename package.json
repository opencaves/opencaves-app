{
  "name": "opencaves-app",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --prefix none \"firebase --debug emulators:start --import=./.emulator-data\"  \"craco  start\"",
    "dev:serve": "craco start",
    "serve": "serve -p 4000 -s build",
    "build": "craco  build",
    "deploy": "firebase deploy --only hosting",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": ">= 18.16.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
